datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model SportsArticle {
  id        String   @id @default(uuid())
  title     String   @db.VarChar(255)
  content   String   @db.Text
  imageUrl  String?
  createdAt DateTime @default(now())
  deletedAt DateTime?
}
